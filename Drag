local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Drag Hub", "Ocean")

-- Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Crash", "Crash", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/22kristina/dhcrash_gen2/main/crash", true))()
end)

MainSection:NewButton("Lock", "Lock", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/farthefarter/e/main/e"))()
end)

MainSection:NewButton("Money Counter", "Money Counter", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/OG5/IkuScripts/main/DH/MoneyCounter', true))()
end)

MainSection:NewKeybind("Turn On/Off", "KeybindInfo", Enum.KeyCode.E, function()
    Library:ToggleUI()
end)


-- Fun Stuff
local FunStuff = Window:NewTab("Fun Stuff")
local FunStuffSection = FunStuff:NewSection("Fun Stuff")

FunStuffSection:NewButton("Mind Control/Telekinesis", "When People Are Knocked Click On Them To Control", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/dawn-spec/dimagbbbbb/main/wow.lua"))()
end)

FunStuffSection:NewButton("Headless", "Headless", function()
    game.Players.LocalPlayer.Character.Head.Transparency = 1
game.Players.LocalPlayer.Character.Head.Transparency = 1
for i,v in pairs(game.Players.LocalPlayer.Character.Head:GetChildren()) do
if (v:IsA("Decal")) then
v.Transparency = 1
end
end
end)

-- Credit
local Credit = Window:NewTab("Credit")
local CreditSection = Credit:NewSection("Creator - Drag")
local CreditSection = Credit:NewSection("Join https://discord.gg/uak9pepZMc")
